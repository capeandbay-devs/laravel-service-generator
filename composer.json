{
  "name": "capeandbay/laravel-service-generator",
  "description": "A service generator for Laravel",
  "license": "MIT",
  "keywords": [
    "service",
    "generator"
  ],
  "type": "library",
  "authors": [
    {
      "name": "",
      "email": ""
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "CapeAndBay\\ServiceGenerator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CapeAndBay\\ServiceGenerator\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CapeAndBay\\ServiceGenerator\\ServiceGeneratorServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
